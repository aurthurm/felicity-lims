{
  "compilerOptions": {
    "strict": true,
    "jsx": "vue",
    "outDir": "./dist",
    "target": "esnext", // es2017, esnext,es6
    "module": "esnext", // commonjs, es6
    "lib": [ // es2015
      "ESNext",
      "DOM"
    ],
    "declaration": false,
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/vite/dist/"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./**/*.ts",
    "./src/**/*.vue",
    "src/App.tsx",
    "src/orig.tsx"
  ],
  "types": [
    "vue-sweetalert2",
    "vite/client",
  ],
}