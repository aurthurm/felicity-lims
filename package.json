{
  "name": "beak-lims",
  "version": "0.2.3",
  "main": "webapp/main.ts",
  "license": "Public",
  "scripts": {
    "webapp:dev": "vite",
    "webapp:dev:watch": "concurrently \"pnpm run webapp:codegen\" \"pnpm run webapp:dev\"",
    "webapp:vf": "vite --force",
    "webapp:build:report": "npx vite-bundle-visualizer",
    "webapp:build:only": "NODE_OPTIONS='--max-old-space-size=4096' vite build",
    "webapp:prettier:check": "prettier --check \"{,webapp/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "webapp:prettier:format": "prettier --write \"{,webapp/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "webapp:lint": "eslint . --ext .js,.ts,.tsx,.vue",
    "webapp:lint:fix": "npm run lint -- --fix",
    "webapp:codegen": "graphql-codegen --config codegen.yml --watch --verbose",
    "server:install": "pip install -e .",
    "server:uv": "uvicorn beak.main:beak",
    "server:uv:watch": "uvicorn beak.main:beak --reload",
    "server:gu": "gunicorn beak.main:beak --workers 5 --worker-class uvicorn.workers.UvicornH11Worker --bind 0.0.0.0:8000 --name beak --access-logfile - --error-logfile - --log-level info --timeout 600",
    "server:gu:watch": "gunicorn beak.main:beak --workers 1 --worker-class uvicorn.workers.UvicornH11Worker --bind 0.0.0.0:8000 --name beak --reload --access-logfile - --error-logfile - --log-level info --timeout 600",
    "server:test": "bash ./beak/scripts/test.sh",
    "server:lint": "bash ./beak/scripts/lint.sh",
    "server:format": "bash ./beak/scripts/format.sh",
    "server:schema:export": "strawberry export-schema beak.api.gql.schema:schema > ./beak/api/gql/schema.graphql",
    "server:schema:export:tofront": "strawberry export-schema beak.api.gql.schema:schema > ./webapp/graphql/schema.graphql",
    "standalone:build": "rm -rf beak/templates/static/* && NODE_OPTIONS='--max-old-space-size=4096' vite build && cp -r dist/** beak/templates/static",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "db:upgrade": "beak-lims upgrade",
    "db:revision": "beak-lims revision",
    "db:updates": "beak-lims updates"
  },
